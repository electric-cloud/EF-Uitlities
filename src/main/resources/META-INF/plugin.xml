<plugin>
  <key>@PLUGIN_KEY@</key>
  <version>@PLUGIN_VERSION@</version>
  <label>@PLUGIN_KEY@</label>
  <description>A set of common procedures for Deploy use-cases</description>
  <help>help.xml</help>
  <author>Electric Cloud</author>
  <authorUrl>mailto:http://electric-cloud.com/support</authorUrl>

  <commander-version min="6.2"/>

  <!-- Specify a category for this plugin; this is used in filtered lists in the Plugin Manager.
       Typically, it is set to System or Step.
   -->
  <category>Deploy</category>
  <depends>EC-Core</depends>
</plugin>
